{
  "compilerOptions": {
    "baseUrl": "./private_set_intersection/javascript",
    "declaration": true,
    "declarationDir": "./private_set_intersection/javascript/tsc-out/types",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "newLine": "LF",
    "noEmitOnError": true,
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "./private_set_intersection/javascript/tsc-out",
    "paths": {
      "psi_*": ["./private_set_intersection/javascript/bin/psi_*"]
    },
    "pretty": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "rootDir": "./private_set_intersection/javascript/src",
    "sourceMap": true,
    "strict": true,
    "target": "ES2020",
    "typeRoots": [
      "./private_set_intersection/javascript/custom_types",
      "./node_modules/@types"
    ]
  },
  "include": ["./private_set_intersection/javascript/src/**/*"],
  "exclude": ["./node_modules/**/*", "./private_set_intersection/javascript/tsc-out/**/*", "./private_set_intersection/javascript/src/__tests__/**/*"]
}
