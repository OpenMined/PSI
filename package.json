{
  "name": "psi-cardinality",
  "version": "0.0.1",
  "description": "PSI cardinality for JS and WASM using Bazel and Emscripten",
  "main": "src/javascript/index.js",
  "license": "Apache-2.0",
  "scripts": {
    "benchmark:js": "RUN_DEMO=js node ./src/javascript/benchmark.js",
    "benchmark:wasm": "RUN_DEMO=wasm node ./src/javascript/benchmark.js",
    "build:client": "yarn build:client:js && yarn build:client:wasm",
    "build:client:js": "bash src/javascript/scripts/build-client-js.sh",
    "build:client:wasm": "bash src/javascript/scripts/build-client-wasm.sh",
    "build:server": "yarn build:server:js && yarn build:server:wasm",
    "build:server:js": "bash src/javascript/scripts/build-server-js.sh",
    "build:server:wasm": "bash src/javascript/scripts/build-server-wasm.sh",
    "build:benchmark": "yarn build:benchmark:js && yarn build:benchmark:wasm",
    "build:benchmark:js": "bash src/javascript/scripts/build-benchmark-js.sh",
    "build:benchmark:wasm": "bash src/javascript/scripts/build-benchmark-wasm.sh",
    "clean": "bazel clean --expunge",
    "submodule:update": "bash src/javascript/scripts/submodule-update.sh",
    "em:update": "bash src/javascript/scripts/em-update.sh",
    "em:init": "bash src/javascript/scripts/em-init.sh"
  }
}
