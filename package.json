{
  "name": "@openmined/psi.js",
  "version": "0.2.1",
  "description": "Private Set Intersection for JavaScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/OpenMined/PSI/tree/master/private_set_intersection/javascript"
  },
  "license": "Apache-2.0",
  "keywords": [
    "psi",
    "psi.js",
    "private set intersection",
    "private join and compute"
  ],
  "main": "combined/wasm/cjs/index.js",
  "browser": "combined/wasm/iife/index.js",
  "module": "combined/wasm/es/index.js",
  "files": [
    "**/*.js"
  ],
  "scripts": {
    "benchmark:js": "RUN_DEMO=js node -r esm private_set_intersection/javascript/benchmark.js",
    "benchmark:wasm": "RUN_DEMO=wasm node -r esm private_set_intersection/javascript/benchmark.js",
    "build": "npm run build:client && npm run build:server && npm run build:combined",
    "build:client": "npm run build:client:js && npm run build:client:wasm",
    "build:client:js": "bash private_set_intersection/javascript/scripts/build-client-js.sh && npm run build:copy",
    "build:client:wasm": "bash private_set_intersection/javascript/scripts/build-client-wasm.sh && npm run build:copy",
    "build:combined": "npm run build:combined:js && npm run build:combined:wasm",
    "build:combined:js": "bash private_set_intersection/javascript/scripts/build-combined-js.sh && npm run build:copy",
    "build:combined:wasm": "bash private_set_intersection/javascript/scripts/build-combined-wasm.sh && npm run build:copy",
    "build:server": "npm run build:server:js && npm run build:server:wasm",
    "build:server:js": "bash private_set_intersection/javascript/scripts/build-server-js.sh && npm run build:copy",
    "build:server:wasm": "bash private_set_intersection/javascript/scripts/build-server-wasm.sh && npm run build:copy",
    "build:benchmark": "npm run build:benchmark:js && npm run build:benchmark:wasm",
    "build:benchmark:js": "bash private_set_intersection/javascript/scripts/build-benchmark-js.sh && npm run build:copy",
    "build:benchmark:wasm": "bash private_set_intersection/javascript/scripts/build-benchmark-wasm.sh && npm run build:copy",
    "build:copy": "bash private_set_intersection/javascript/scripts/build-copy.sh",
    "build:proto": "bash private_set_intersection/javascript/scripts/build-proto.sh",
    "postbuild": "node -r esm private_set_intersection/javascript/version.js",
    "prerollup": "bash private_set_intersection/javascript/scripts/prerollup.sh",
    "rollup": "rollup -c",
    "postrollup": "bash private_set_intersection/javascript/scripts/postrollup.sh",
    "prepublishOnly": "echo 'Please run \"npm run publish\" instead' && exit 1",
    "publish:test": "npm --dry-run publish --access=public private_set_intersection/javascript/dist/openmined-psi.js-*.tgz",
    "publish": "npm publish --access=public private_set_intersection/javascript/dist/openmined-psi.js-*.tgz",
    "clean": "bazel clean --expunge && rm -rf private_set_intersection/javascript/bin/* && rm -rf private_set_intersection/javascript/tsc-out/* && rm -rf private_set_intersection/javascript/dist/*",
    "compile": "tsc",
    "format": "prettier --write private_set_intersection/javascript/src",
    "lint": "eslint --fix 'private_set_intersection/javascript/src/**/*.{js,ts}'",
    "test": "jest private_set_intersection/javascript/src/",
    "test:js": "jest private_set_intersection/javascript/tsc-out/__tests__/",
    "coverage": "jest private_set_intersection/javascript/src --coverage",
    "submodule:update": "bash private_set_intersection/javascript/scripts/submodule-update.sh",
    "em:update": "bash private_set_intersection/javascript/scripts/em-update.sh",
    "em:init": "bash private_set_intersection/javascript/scripts/em-init.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.4.0",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/google-protobuf": "^3.7.2",
    "@types/jest": "^26.0.7",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "buffer": "^5.6.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-jsdoc": "^30.0.3",
    "eslint-plugin-prettier": "^3.1.4",
    "esm": "^3.2.25",
    "jest": "^26.1.0",
    "path": "^0.12.7",
    "prettier": "^2.0.5",
    "rollup": "^2.23.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^6.1.0",
    "ts-jest": "^26.1.3",
    "ts-protoc-gen": "^0.12.0",
    "typescript": "^3.9.7"
  }
}
